; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="excela"
run/main_scene="res://Prefabs/menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"
config/size/mode=0

[autoload]

Globals="*res://Scripts/Globals.gd"

[display]

window/size/viewport_width=240
window/size/viewport_height=427
window/stretch/mode="viewport"

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Prefabs/": "orange",
"res://Scenes/": "yellow",
"res://Scripts/": "green"
}

[layer_names]

2d_render/layer_1="Player"
2d_render/layer_2="Coffe"
2d_render/layer_3="Boss"
2d_render/layer_4="Object"
2d_physics/layer_1="Player"
2d_physics/layer_2="Coffe"
2d_physics/layer_3="Boss"
2d_physics/layer_4="Obj"

[rendering]

textures/canvas_textures/default_texture_filter=0
